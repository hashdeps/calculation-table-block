{
  "blockprotocol": {
    "displayName": "Divider",
    "icon": "public/divider.svg",
    "image": "public/preview.svg",
    "examples": [
      {
        "color": "black",
        "height": "2px"
      }
    ],
    "protocol": "0.1"
  },
  "private": true,
  "scripts": {
    "prestart": "dotnet tool restore",
    "dev": "dotnet fable watch ./src -e .fs.js -o dist/build -s --run webpack-dev-server",
    "prebuild": "dotnet tool restore",
    "build": "dotnet fable ./src -e .fs.js -o dist/build && webpack",
    "serve": "concurrently -n build,serve 'dotnet fable watch ./src -e .fs.js -o dist/build --runWatch webpack' 'npm:http-serve'",
    "http-serve": "http-server dist/out -p 7777 --cors",
    "test:live": "dotnet fable watch tests -e .fs.js -o dist/tests --run webpack-dev-server --config ./webpack.tests.js",
    "build:test": "dotnet fable tests -e .fs.js -o dist/tests && webpack --config webpack.tests.js",
    "pretest": "dotnet tool restore",
    "test": "rimraf dist/mocha-tests && dotnet fable tests -e .fs.mjs -o dist/mocha-tests && mocha dist/mocha-tests"
  },
  "dependencies": {
    "blockprotocol": "*",
    "date-fns": "^2.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime": "^7.17.9",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.1.0",
    "copy-webpack-plugin": "10.2.4",
    "core-js": "^3.8.2",
    "css-loader": "^5.2.7",
    "dotenv-webpack": "^6.0.0",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "graceful-fs": "^4.2.4",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "mini-css-extract-plugin": "^1.3.4",
    "mocha": "^9.2.2",
    "mock-block-dock": "0.0.8",
    "rimraf": "^3.0.2",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "terser": "^4.8.0",
    "webpack": "^5.72.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
